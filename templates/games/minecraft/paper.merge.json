{
    "merge": "minecraft.json",
    "data": {
        "title": "PaperMC",
        "description": "PaperMC Minecraft Server",
        "categories": ["Games", "Minecraft", "amd64", "arm64"],
        "logo": "$ICON_PATH/paper.png",
        "env": [
            {
                "name": "SERVER_TYPE",
                "default": "PAPER",
                "preset": true
            },
            {
                "label": "Version",
                "name": "VERSION",
                "preset": false,
                "default": "$LATEST_VERSION"
            },
            {
                "label": "Spiget Resource ID",
                "name": "SPIGET_RESOURCES",
                "preset": false
            }
        ]
    }
}
