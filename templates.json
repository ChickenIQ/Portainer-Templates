{
    "version": "2",
    "templates": [
        {
            "type": 2,
            "title": "PaperMC",
            "description": "PaperMC Minecraft Server",
            "categories": ["Minecraft"],
            "platform": "linux",
            "logo": "data:image/png;base64,",
            "repository": {
                "url": "https://github.com/ChickenIQ/portainer-templates",
                "stackfile": "stacks/minecraft/paper.yml"
            },
            "env": [
                {
                    "label": "Java Version",
                    "name": "JAVA_VERSION",
                    "select": [
                        { "text": "Java 17", "value": "java17", "default": "true" },
                        { "text": "Java 8", "value": "java8-multiarch" }
                    ]
                },
                {
                    "label": "Server Address",
                    "name": "HOST"
                },
                {
                    "label": "Node Hostname",
                    "name": "NODE_HOSTNAME",
                    "default": "Main"
                },
                {
                    "label": "Version",
                    "name": "VERSION",
                    "default": "1.20.1"
                },
                {
                    "label": "Spiget Resource ID",
                    "name": "SPIGET_RESOURCES"
                },
                {
                    "label": "MOTD",
                    "name": "MOTD",
                    "default": "ChickenIQ's Server"
                },
                {
                    "label": "Operators",
                    "name": "OPS",
                    "default": "ChickenIQ"
                },
                {
                    "label": "Whitelist",
                    "name": "WHITELIST",
                    "default": "ChickenIQ"
                },
                {
                    "label": "Max Players",
                    "name": "MAX_PLAYERS",
                    "default": "69"
                },
                {
                    "label": "Difficulty",
                    "name": "DIFFICULTY",
                    "select": [
                        { "text": "Peaceful", "value": "peaceful" },
                        { "text": "Easy", "value": "easy" },
                        { "text": "Normal", "value": "normal", "default": "true" },
                        { "text": "Hard", "value": "hard" }
                    ]
                },
                {
                    "label": "Online Mode",
                    "name": "ONLINE_MODE",
                    "select": [
                        { "text": "True", "value": "true", "default": "true" },
                        { "text": "False", "value": "false" }
                    ]
                },
                {
                    "label": "Allow Flight",
                    "name": "ALLOW_FLIGHT",
                    "select": [
                        { "text": "True", "value": "true", "default": "true" },
                        { "text": "False", "value": "false" }
                    ]
                },
                {
                    "label": "Enable Command Blocks",
                    "name": "ENABLE_COMMAND_BLOCK",
                    "select": [
                        { "text": "True", "value": "true", "default": "true" },
                        { "text": "False", "value": "false" }
                    ]
                },
                {
                    "label": "Override Server Properties",
                    "name": "OVERRIDE_SERVER_PROPERTIES",
                    "select": [
                        { "text": "True", "value": "true", "default": "true" },
                        { "text": "False", "value": "false" }
                    ]
                },
                {
                    "label": "Override Whitelist",
                    "name": "OVERRIDE_WHITELIST",
                    "select": [
                        { "text": "True", "value": "true", "default": "true" },
                        { "text": "False", "value": "false" }
                    ]
                },
                {
                    "label": "Override Icon",
                    "name": "OVERRIDE_ICON",
                    "select": [
                        { "text": "True", "value": "true", "default": "true" },
                        { "text": "False", "value": "false" }
                    ]
                },
                {
                    "label": "Max Memory",
                    "name": "MAX_MEMORY",
                    "default": "8G"
                },
                {
                    "label": "Init Memory",
                    "name": "INIT_MEMORY",
                    "default": "1G"
                },
                {
                    "label": "Server Icon",
                    "name": "ICON",
                    "default": "https://cdn.discordapp.com/attachments/851523458524643329/1122529789920694272/hKJVi53.png"
                },
                {
                    "label": "Exec Directly",
                    "name": "EXEC_DIRECTLY",
                    "select": [
                        { "text": "True", "value": "true", "default": "true" },
                        { "text": "False", "value": "false" }
                    ]
                }
            ]
        }
    ]
}
